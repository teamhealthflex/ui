{
  "version": "0.0.0",
  "name": "@teamhealthflex/ui",
  "source": "./src/index.tsx",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "description": "minimal react native ui library",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "scripts": {
    "test": "jest",
    "typecheck": "tsc",
    "clean": "del-cli lib",
    "prepare": "bob build",
    "release": "release-it",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "example": "yarn workspace @teamhealthflex/ui-example"
  },
  "files": [
    "src",
    "lib",
    "ios",
    "cpp",
    "android",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "react-native.config.js",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "keywords": [
    "ios",
    "android",
    "react-native"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamhealthflex/ui.git"
  },
  "author": "nirajgeorgianflex <niraj.kishore@healthflex.in> (http://healthflex.in/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teamhealthflex/ui/issues"
  },
  "homepage": "https://github.com/teamhealthflex/ui#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@teamhealthflex"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.5.0",
    "@react-native/eslint-config": "^0.73.1",
    "@release-it/conventional-changelog": "^9.0.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.44",
    "babel-jest": "^29.7.0",
    "babel-plugin-syntax-hermes-parser": "^0.25.1",
    "commitlint": "^17.0.2",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-testing-library": "^7.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "18.3.1",
    "react-native": "0.76.3",
    "react-native-builder-bob": "^0.35.2",
    "react-test-renderer": "18.3.1",
    "release-it": "^17.10.0",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@3.6.1",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "codegen",
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "esm": true
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "RNUiSpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "outputDir": {
      "ios": "ios/generated",
      "android": "android/generated"
    },
    "android": {
      "javaPackageName": "com.teamhealthflex.ui"
    }
  },
  "create-react-native-library": {
    "languages": "js",
    "type": "library",
    "version": "0.45.3"
  },
  "dependencies": {
    "@candlefinance/faster-image": "^1.7.2",
    "@expo/vector-icons": "^14.0.4",
    "expo-linear-gradient": "^14.0.1",
    "expo-modules-core": "^2.1.2",
    "lottie-react-native": "^7.1.0",
    "react-hook-form": "^7.54.2",
    "react-native-reanimated": "^3.16.6",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^5.0.0",
    "react-native-svg": "^15.10.1"
  }
}
